<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.owner.courseplanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme" >
        <activity
            android:name="com.example.owner.courseplanner.SelectCompletedCourses"
            android:label="@string/title_activity_select_completed_courses" >
        </activity>
        <activity
            android:name="com.example.owner.courseplanner.ChooseCourses"
            android:label="@string/title_activity_choose_courses" >
        </activity>
        <activity
            android:name="com.example.owner.courseplanner.MyCourses"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.owner.courseplanner.CourseArchive"
            android:label="@string/title_activity_course_archive" >
        </activity>
        <activity
            android:name="com.example.owner.courseplanner.AppLauncher"
            android:label="@string/title_activity_app_launcher"
            android:noHistory="true"
            android:theme="@style/activityTransparent" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>